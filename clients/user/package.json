  "scripts": {
    "ng": "ng",
    "config": "node ./set-env.js",
    "start": "ng serve --port 80 --host 0.0.0.0 --disable-host-check --poll 500",
    "serve:local": "npm run config && ng serve --port 4201",
    "serve:docker": "node ./set-env.js && ng run universal-demo:serve-docker --port 80 --host 0.0.0.0 --disable-host-check --poll 500",
    "serve:ssr:local": "npm run config && ng run universal-demo:serve-ssr --port 80",
    "serve:ssr:docker": "node ./set-env.js && ng run universal-demo:serve-ssr --port 80 --proxy-config proxy.conf.json",
    "ssr": "ng run universal-demo:serve-ssr ",
    "build": "ng build",
    "build:server": "ng run universal-demo:server:production",
    "build:prod": "ng build --configuration production",
    "build:universal": "npm run config && ng run universal-demo:build:production && ng run universal-demo:server:production",
    "server": "node dist-server/main.js",
    "dev:ssr": "npm run config && ng run universal-demo:serve-ssr",
    "build:ssr": "ng build --configuration production && ng run universal-demo:server:production"
  },
