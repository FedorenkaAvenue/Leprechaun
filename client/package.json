{
  "name": "universal-demo",
  "version": "11.0.0",
  "license": "MIT",
  "engines": {
    "node": "16.15.0",
    "npm": "8.3.*",
    "yarn": "1.22.*"
  },
  "scripts": {
    "ng": "ng",
    "config:dev": "cross-env NODE_ENV=dev node ./set-env.js",
    "config:prod": "cross-env NODE_ENV=production node ./set-env.js",
    "start": "ng serve --port 80 --host 0.0.0.0 --disable-host-check --poll 500",
    "serve:local": "npm run config:dev && ng serve  --port 4201",
    "serve:docker": "npm run config:prod && ng serve --port 80 --host 0.0.0.0 --disable-host-check --poll 500",
    "ssr": "ng run universal-demo:serve-ssr ",
    "ssr:watch": "ng run universal-demo:serve-ssr:dev",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "build": "ng build",
    "build:server": "ng run universal-demo:server:production",
    "build:prod": "ng build --configuration production",
    "build:universal": "npm run config:prod && ng run universal-demo:build:production && ng run universal-demo:server:production",
    "server": "node dist-server/main.js",
    "test": "ng test",
    "test:ci": "cross-env CI=true ng test",
    "test:ssr": "run-p test:ssr:*",
    "test:ssr:server": "cross-env TEST=true node dist-server/main.js",
    "test:ssr:script": "node ./node_modules/npm-delay 2000 && ts-mocha test/*.spec.ts test/exit.ts --timeout 15000",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "bundle-report:prod": "ng build --stats-json --configuration production && webpack-bundle-analyzer dist/stats.json",
    "dev:ssr": "ng run universal-demo:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build --configuration production && ng run universal-demo:server:production",
    "prerender": "ng run universal-demo:prerender",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@angular/service-worker": "13.3.11",
    "@gilsdav/ngx-translate-router": "~4.0.0",
    "@gorniv/ngx-universal": "2.2.2",
    "@ng-select/ng-select": "^6.1.0",
    "@ngneat/transloco": "^2.22.0",
    "@nguniversal/common": "11.2.1",
    "@nguniversal/express-engine": "11.2.1",
    "@ngx-meta/core": "^8.0.2",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "cookie-parser": "1.4.5",
    "core-js": "3.11.0",
    "express": "4.17.1",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular/cli": "13.3.8",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@angular/platform-server": "13.3.11",
    "@nguniversal/builders": "11.2.1",
    "@types/chai": "4.2.17",
    "@types/express": "4.17.3",
    "@types/jasmine": "3.6.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "15.0.1",
    "chai": "4.3.4",
    "codelyzer": "6.0.1",
    "cross-env": "7.0.3",
    "dotenv": "^16.0.1",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.16",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.3.2",
    "nodemon": "2.0.7",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-tslint": "0.4.2",
    "protractor": "7.0.0",
    "source-map-support": "0.5.19",
    "ssri": "8.0.1",
    "ts-mocha": "8.0.0",
    "ts-node": "9.1.1",
    "tslint": "~6.1.3",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "4.6.0"
  }
}
